{
  "name": "ichhabrecht/psr7-middleware-yaml-parser",
  "description": "PSR-7 middleware that parses Yaml files to ServerRequest attribute",
  "keywords": [
    "PSR-7",
    "middleware",
    "yaml"
  ],
  "homepage": "https://github.com/IchHabRecht/psr7-middleware-yaml-parser",
  "authors": [
    {
      "name": "Nicole Cordes",
      "role": "Developer",
      "homepage": "https://twitter.com/IchHabRecht"
    }
  ],
  "license": [
    "MIT"
  ],
  "require": {
    "psr/http-message": "^1.0",
    "symfony/yaml": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8.0",
    "mikey179/vfsStream": "~1.6.0"
  },
  "autoload": {
    "psr-4": {
      "IchHabRecht\\Psr7MiddlewareYamlParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IchHabRecht\\Psr7MiddlewareYamlParser\\Tests\\": "tests/"
    }
  }
}
